{
  "name": "zignaly-ui",
  "version": "0.0.1",
  "description": "A library of interface resources designed by Zignaly.",
  "main": "lib/index.js",
  "scripts": {
    "storybook": "start-storybook -p 6006 -s ./src",
    "prebuild:storybook": "npm run test:generate-output",
    "build:storybook": "build-storybook -c .storybook -o build/",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test": "jest -u",
    "build": "tsc && tsc-alias && npm run copy-files",
    "copy-files": "cp -R src/assets lib/assets",
    "lint": "eslint --fix src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write src --config ./.prettierrc",
    "chromatic": "npx chromatic --project-token=e5c345734579"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zignaly-open/react-component-library"
  },
  "keywords": [],
  "author": "Zignaly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zignaly-open/react-component-library/issues"
  },
  "homepage": "https://github.com/zignaly-open/react-component-library#readme",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-links": "^6.4.20",
    "@storybook/react": "^6.4.20",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.1",
    "@types/react-table": "^7.7.10",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "chromatic": "^6.5.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.7",
    "extract-loader": "^5.1.0",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "jest-dom": "^4.0.0",
    "lint-staged": "12.3.4",
    "prettier": "^2.6.2",
    "storybook-addon-designs": "^6.2.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@metamask/jazzicon": "^2.0.0",
    "@mui/icons-material": "^5.6.0",
    "@mui/material": "^5.6.0",
    "@mui/styled-engine-sc": "^5.5.2",
    "@storybook/addon-jest": "^6.4.21",
    "@storybook/addons": "^6.4.20",
    "@storybook/theming": "^6.4.20",
    "date-fns": "^2.28.0",
    "ethers": "^5.6.2",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loader-spinner": "^6.0.0-0",
    "react-number-format": "^4.9.1",
    "react-spring": "^9.4.4",
    "react-table": "^7.7.0",
    "react-use": "^17.3.2",
    "styled-components": "^5.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run format"
    ]
  }
}
