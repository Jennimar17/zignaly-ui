{
  "name": "zignaly-ui",
  "version": "0.0.4",
  "description": "A library of interface resources designed by Zignaly.",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "scripts": {
    "storybook": "start-storybook -p 6006 -s ./src",
    "prebuild:storybook": "npm run test:generate-output",
    "build:storybook": "build-storybook -c .storybook -o build/",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test": "jest -u",
    "build": "npm run clean && webpack --mode=production --node-env=production",
    "buildTS": "tsc && tsc-alias && npm run copy-files",
    "buildRollup": "npm run clean && rollup -c",
    "clean": "rm -rf lib",
    "copy-files": "cp -R src/assets lib/assets",
    "lint": "eslint --fix src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write src --config ./.prettierrc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zignaly-open/react-component-library"
  },
  "keywords": [],
  "author": "Zignaly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zignaly-open/react-component-library/issues"
  },
  "homepage": "https://github.com/zignaly-open/react-component-library#readme",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@metamask/jazzicon": "^2.0.0",
    "@mui/icons-material": "^5.4.2",
    "@mui/styled-engine-sc": "^5.5.2",
    "@storybook/addons": "^6.4.20",
    "@storybook/theming": "^6.4.20",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "date-fns": "^2.28.0",
    "ethers": "^5.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-syntax-jsx": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.5.2",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-jest": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-options": "^5.3.21",
    "@storybook/react": "^6.4.19",
    "@storybook/theming": "^6.4.19",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/react-table": "^7.7.9",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.7",
    "fork-ts-checker-webpack-plugin": "^7.2.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "jest-dom": "^4.0.0",
    "lint-staged": "12.3.4",
    "prettier": "^2.5.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loader-spinner": "^6.0.0-0",
    "react-number-format": "^4.9.1",
    "react-spring": "^9.4.4",
    "react-table": "^7.7.0",
    "react-test-renderer": "^17.0.2",
    "react-use": "^17.3.1",
    "rollup-plugin-svg": "^2.0.0",
    "storybook-addon-designs": "^6.2.1",
    "storybook-dark-mode": "^1.0.9",
    "styled-components": "^5.3.3",
    "ts-loader": "^9.2.8",
    "tsc-alias": "^1.6.4",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.5",
    "typescript-styled-plugin": "^0.18.2",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2"
  },
  "peerDependencies": {
    "@mui/material": ">= 5",
    "react": ">= 16.14",
    "react-dom": ">= 16.14"
  },
  "jest": {
    "rootDir": ".",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^.+\\.svg$": "jest-svg-transformer",
      "^@root(.*)$": "<rootDir>/src$1",
      "^@assets(.*)$": "<rootDir>/src/assets$1",
      "^@hooks(.*)$": "<rootDir>/src/hooks$1",
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@utils(.*)$": "<rootDir>/src/utils$1",
      "^@types(.*)$": "<rootDir>/src/types$1"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run format"
    ]
  }
}